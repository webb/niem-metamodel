    <Datatype s:uri="http://www.w3.org/2001/XMLSchema#nonNegativeInteger" xsi:nil="true"/>
  </DataProperty>
  <DataProperty s:uri="http://reference.niem.gov/specification/metamodel/5.0alpha2#maxOccursQuantity">
    <Name>maxOccursQuantity</Name>
