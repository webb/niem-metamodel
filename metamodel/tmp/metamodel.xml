                        </HasDataProperty>
                        <HasDataProperty mm:minOccursQuantity="1" mm:maxOccursQuantity="1">
                          <DataProperty s:uri="http://reference.niem.gov/specification/metamodel/5.0alpha2#minOccursQuantity" xsi:nil="true"/>
                        </HasDataProperty>
                        <HasDataProperty mm:minOccursQuantity="1" mm:maxOccursQuantity="1">
